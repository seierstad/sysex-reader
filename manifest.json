{
 "$schema": "https://json.schemastore.org/web-manifest-combined.json",
 "name": "Sysex reader",
 "short_name": "sysex",
 "version": "0.0.1",
 "description": "View MIDI system exclusive (sysex) data",
 "author": "Erik E. Seierstad",
 "id": "sysex-reader",
 "display_override": ["fullscreen", "minimal-ui"],
 "display": "standalone",
 "orientation": "any",
 "start_url": "/",
 "theme_color": "#0fb0a0",
 "background_color": "#fff",
 "icons": [
  {
    "src": "\/images\/logo.svg",
    "sizes": "any",
    "type": "image/svg+xml",
    "purpose": "maskable"
  },
  {
   "src": "\/images\/android-icon-36x36.png",
   "sizes": "36x36",
   "type": "image\/png",
   "density": "0.75"
  },
  {
   "src": "\/images\/android-icon-48x48.png",
   "sizes": "48x48",
   "type": "image\/png",
   "density": "1.0"
  },
  {
   "src": "\/images\/android-icon-72x72.png",
   "sizes": "72x72",
   "type": "image\/png",
   "density": "1.5"
  },
  {
   "src": "\/images\/android-icon-96x96.png",
   "sizes": "96x96",
   "type": "image\/png",
   "density": "2.0"
  },
  {
   "src": "\/images\/android-icon-144x144.png",
   "sizes": "144x144",
   "type": "image\/png",
   "density": "3.0"
  },
  {
   "src": "\/images\/android-icon-192x192.png",
   "sizes": "192x192",
   "type": "image\/png",
   "density": "4.0"
  },
  {
   "src": "\/images\/android-icon-512x512.png",
   "sizes": "512x512",
   "type": "image\/png",
   "density": "4.0"
  }
 ]
}